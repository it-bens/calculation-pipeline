{
  "name": "it-bens/calculation-pipeline",
  "description": "",
  "type": "project",
  "authors": [
    {
      "name": "Martin Bens",
      "email": "martin.bens@it-bens.de"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-bcmath": "*",
    "moneyphp/money": "^3.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-phpunit": "^1.2",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "allow-plugins": {
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ITB\\CalculationPipeline\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ITB\\CalculationPipeline\\Tests\\": "tests/"
    }
  },
  "extra": {
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  }
}
